<?xml version="1.0"?>
<robot name="wall">

  <!-- A root link called 'world' -->
  <link name="world"/>

  <link name="wall_link">
    <visual>
      <geometry>
        <box size="2 0.1 1"/>
      </geometry>
      <material name="grey">
        <color rgba="0.5 0.5 0.5 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <box size="2 0.1 1"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="100"/>
      <inertia ixx="10" ixy="0" ixz="0" iyy="10" iyz="0" izz="10"/>
    </inertial>
  </link>

  <!-- Fixed joint connecting wall_link to world -->
  <joint name="wall_fixed_joint" type="fixed">
    <parent link="world"/>
    <child link="wall_link"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
  </joint>

</robot>
